<h1>Workout Show page</h1>
<p><%= @workout.date %></p>
<p><%= @workout.time %></p>
<p><%= @workout.workout_type %></p>
<p><%= @workout.hours %>:<%= @workout.minutes %><%= @workout.seconds %></p>
<br>
<hr>
<% @exercises.each do |exercise| %>
<p><%= exercise.name %></p>
<% end %>
<hr>
<% @cardios.each do |cardio| %>
<p><%= cardio.name %> - <%= cardio.hours %>:<%= cardio.minutes %>:<%= cardio.seconds %></p>
<% end %>
